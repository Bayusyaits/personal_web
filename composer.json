{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "dingo/api": "2.0.0-alpha1",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "htmlmin/htmlmin": "^5.6",
        "laravel/framework": "5.6.*",
        "laravel/passport": "~4.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "league/fractal": "^0.17.0",
        "logaretm/transformers": "^0.2.5",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "optimus/api-consumer": "0.2.*",
        "pragmarx/firewall": "^2.2",
        "tymon/jwt-auth": "dev-develop",
        "ven7ura/laravel-html5-boilerplate": "^0.6.6"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "^7",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "files":[
            "libs/general_function.php",
            "libs/variable_function.php",
            "app/Support/helpers.php",
            "libs/alias.php",
            "libs/api.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Dingo\\Api\\": "src/",
            "App\\": "app/"
        },
        "psr-0": {
           "app" :[ 
               "libs/",
               "app/Helpers",
               "app/Models",
               "app/Modules",
               "app/Repository"
           ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Api\\Tests\\": "tests/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ],
        "providers": [
            "Dingo\\Api\\Provider\\LaravelServiceProvider"
        ],
        "aliases": {
            "API": "Dingo\\Api\\Facade\\API"
        }
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
